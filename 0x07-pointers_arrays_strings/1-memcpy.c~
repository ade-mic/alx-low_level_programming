#include <stdio.h>
#include <string.h>
#include "main.h"
/**
 * *_memcpy - a function that copy a block of memoey from a location to another
 *
 * @dest: destination memeory location
 * @src: source memory location
 * @n: size of memory location
 *
 * Return: A pointer to dest
 */

char *_memcpy(char *dest, char *src, unsigned int n)
{
	int i;

	for (i = 0; n > 0 ; n--)
	{
		*(dest + i) = *(s + i);
		i++;
	}
	return (dest);
}
